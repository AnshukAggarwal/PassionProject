@model AutoCareInc.Models.InvoiceItem

@{
    ViewBag.Title = "Show";
}

<h2>Details for @Model.InvoiceItemName</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            Name:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.InvoiceItemName)
        </dd>

        <dt>
            Price:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.InvoiceItemPrice)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.InvoiceItemID }) |
    @Html.ActionLink("Back to List", "List")
</p>
